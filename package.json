  "dependencies": {
    "axios": "^0.21.1",
    "next": "10.0.9",
    "node-html-parser": "^5.3.3",
    "normalize.css": "^8.0.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-query": "^3.13.0",
    "react-textarea-autosize": "^8.3.2",
    "sass": "^1.32.13"
  },
  "devDependencies": {
    "@types/node": "^14.14.35",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^17.0.4",
    "prettier": "2.8.2",
    "typescript": "^4.2.3"
