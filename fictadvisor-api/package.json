{
  "name": "fictadvisor-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "dotenv -e .development.env -- cross-env NODE_ENV=development nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/src/main",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "test:unit": "cross-env NODE_ENV=testing dotenv -e .testing.env -- jest -c jest.unit.config.json",
    "test:integration:fictadvisor": "cross-env NODE_ENV=testing dotenv -e .testing.env -- jest -c jest.integration.fictadvisor.config.json -i",
    "test:integration:cohorta": "cross-env NODE_ENV=testing dotenv -e .testing.env -- jest -c jest.integration.cohorta.config.json -i",
    "test:seed:fictadvisor": "dotenv -e .testing.env -- prisma db seed -- --project fictadvisor",
    "test:seed:cohorta": "dotenv -e .testing.env -- prisma db seed -- --project cohorta",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "integration:full:fictadvisor": "docker compose up -d && yarn migrate:test:fictadvisor && yarn test:seed:fictadvisor && yarn test:integration:fictadvisor && docker compose down",
    "integration:full:cohorta": "docker compose up -d && yarn migrate:test:cohorta && yarn test:seed:cohorta && yarn test:integration:cohorta && docker compose down",
    "generate:fictadvisor": "npx prisma generate --schema ./prisma/fictadvisor/schema.prisma",
    "generate:cohorta": "npx prisma generate --schema ./prisma/cohorta/schema.prisma",
    "generate:full": "yarn generate:fictadvisor && yarn generate:cohorta",
    "migrate:dev:fictadvisor": "dotenv -e .development.env -- npx prisma migrate dev --schema ./prisma/fictadvisor/schema.prisma",
    "migrate:sql:fictadvisor": "yarn migrate:dev:fictadvisor --create-only --name",
    "migrate:test:fictadvisor": "dotenv -e .testing.env -- npx prisma migrate deploy --schema ./prisma/fictadvisor/schema.prisma",
    "migrate:dev:cohorta": "dotenv -e .development.env -- npx prisma migrate dev --schema ./prisma/cohorta/schema.prisma",
    "migrate:sql:cohorta": "yarn migrate:dev:cohorta --create-only --name",
    "migrate:test:cohorta": "dotenv -e .testing.env -- npx prisma migrate deploy --schema ./prisma/cohorta/schema.prisma"
  },
  "dependencies": {
    "@automapper/classes": "^8.8.1",
    "@automapper/core": "^8.8.1",
    "@automapper/nestjs": "^8.8.1",
    "@fictadvisor/utils": "*",
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/common": "^9.2.1",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.2.1",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/microservices": "^10.4.15",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.2.1",
    "@nestjs/schedule": "^3.0.1",
    "@nestjs/swagger": "^6.3.0",
    "@nestjs/terminus": "^10.2.3",
    "@nestjs/typeorm": "^9.0.1",
    "@nestjs/websockets": "^10.4.15",
    "@prisma/client": "4.16.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsdom": "^21.1.0",
    "@types/nodemailer": "^6.4.7",
    "@types/passport-local": "^1.0.34",
    "@vendia/serverless-express": "^4.12.6",
    "axios": "^1.2.1",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.2.1",
    "firebase": "^10.13.1",
    "firebase-admin": "^12.4.0",
    "handlebars": "^4.7.7",
    "jsdom": "^22.0.0",
    "luxon": "^3.4.4",
    "nodemailer": "^6.9.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "prisma": "4.16.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.1",
    "rxjs": "^7.6.0",
    "uuid": "^9.0.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.1.5",
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/testing": "^9.2.1",
    "@types/cookie-parser": "^1.4.7",
    "@types/cron": "^2.0.1",
    "@types/dateformat": "^5.0.0",
    "@types/express": "^4.17.15",
    "@types/jest": "^29.5.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.2.5",
    "@types/passport-jwt": "^3.0.8",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "5.59.7",
    "@typescript-eslint/parser": "5.59.7",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.1",
    "supertest": "^6.3.3",
    "ts-jest": "29.1.0",
    "ts-loader": "^9.4.2",
    "ts-node": "10.9.1",
    "tsconfig-paths": "^4.1.1",
    "typescript": "5.0.4"
  }
}
